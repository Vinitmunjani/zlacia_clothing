{
    "version":2,
    "builds":[
        {
            "src":"zlacia_clothing/wsgi.py",
            "use":"@vercel/python",
            "config":{"maxLambdaSize":"15mb","runitime":"python3.9"}
        },
        {
            "src":"build_files.sh",
            "use":"@vercel/static-build",
            "config":{
                "distDir":"/staticfiles_build"
            }
        }
    ],
    "routes": [
        {
            "src":"/static/(.*)",
            "dest":"/static/$1"
                },
                {
                    "src":"/(.*)",
                    "dest":"zlacia_clothing/wsgi.py"
                }
    ]
}